{
  "name": "ajala-ai-sdk",
  "version": "1.0.0",
  "description": "A unified JavaScript SDK for seamless AI provider integration",
  "main": "dist/index.js",
  "module": "dist/index.esm.js",
  "types": "dist/index.d.ts",
  "files": [
    "dist",
    "README.md",
    "LICENSE"
  ],
  "scripts": {
    "build": "rollup -c",
    "build:watch": "rollup -c -w",
    "dev": "rollup -c -w",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage",
    "lint": "eslint src/**/*.ts",
    "lint:fix": "eslint src/**/*.ts --fix",
    "format": "prettier --write src/**/*.ts",
    "type-check": "tsc --noEmit",
    "clean": "rimraf dist",
    "prepublishOnly": "npm run clean && npm run build",
    "docs": "typedoc src/index.ts"
  },
  "keywords": [
    "ai",
    "sdk",
    "claude",
    "openai",
    "typescript",
    "unified-api",
    "ai-integration"
  ],
  "author": "Oluwaferanmi Adeniji",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/spiderocious/ajala-ai-sdk.git"
  },
  "bugs": {
    "url": "https://github.com/spiderocious/ajala-ai-sdk/issues"
  },
  "homepage": "https://github.com/spiderocious/ajala-ai-sdk#readme",
  "dependencies": {
    "@anthropic-ai/sdk": "^0.65.0",
    "openai": "^6.0.0"
  },
  "devDependencies": {
    "@rollup/plugin-commonjs": "^25.0.7",
    "@rollup/plugin-node-resolve": "^15.2.3",
    "@rollup/plugin-typescript": "^11.1.5",
    "@types/jest": "^29.5.5",
    "@types/node": "^20.6.0",
    "@typescript-eslint/eslint-plugin": "^6.7.0",
    "@typescript-eslint/parser": "^6.7.0",
    "eslint": "^8.49.0",
    "eslint-config-prettier": "^9.0.0",
    "eslint-plugin-prettier": "^5.0.0",
    "jest": "^29.7.0",
    "prettier": "^3.0.3",
    "rimraf": "^5.0.1",
    "rollup": "^3.29.4",
    "rollup-plugin-terser": "^7.0.2",
    "ts-jest": "^29.1.1",
    "tslib": "^2.6.2",
    "typedoc": "^0.25.1",
    "typescript": "^5.2.2"
  },
  "peerDependencies": {
    "typescript": ">=4.9.0"
  },
  "engines": {
    "node": ">=18.0.0"
  }
}